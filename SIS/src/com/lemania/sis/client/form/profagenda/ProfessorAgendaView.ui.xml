<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel>
    	<g:Label text="Les horaires des enseignants" styleName="pageTitle" horizontalAlignment="ALIGN_CENTER"/>
		<g:VerticalPanel width="100%" spacing="5" horizontalAlignment="ALIGN_CENTER" ui:field="pnlPrincipal">
			<g:VerticalPanel width="100%" spacing="10" horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel width="80%" horizontalAlignment="ALIGN_LEFT" spacing="5" styleName="toolbar">
				<g:Label text="Professeur :" horizontalAlignment="ALIGN_RIGHT"/>
				<g:ListBox ui:field="lstProfs"/>
				</g:HorizontalPanel>
				<g:FlexTable width="100%" ui:field="tblAgenda" borderWidth="0"/>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
