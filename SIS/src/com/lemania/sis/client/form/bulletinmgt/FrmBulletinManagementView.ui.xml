<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%">
			<g:Label text="ATTRIBUTION PERSONNALISEE"
				horizontalAlignment="ALIGN_CENTER" styleName="pageTitle" />
			<g:HorizontalPanel width="100%" spacing="5" height="">
				<g:cell width="20%">
					<g:VerticalPanel width="100%" styleName="subSection"
						height="670px">
						<g:Label text="Choisir un élève" styleName="sectionTitle" />
						<g:Grid width="100%" cellPadding="2" cellSpacing="2">
							<g:row>
								<g:customCell>
									<g:Label text="Ecole :" />
								</g:customCell>
								<g:customCell>
									<g:ListBox width="170px" ui:field="lstEcoles" />
								</g:customCell>
							</g:row>
							<g:row>
								<g:customCell>
									<g:Label text="Programme :" />
								</g:customCell>
								<g:customCell>
									<g:ListBox width="170px" ui:field="lstProgrammes" />
								</g:customCell>
							</g:row>
							<g:row>
								<g:customCell>
									<g:Label text="Classe :" />
								</g:customCell>
								<g:customCell>
									<g:ListBox width="170px" ui:field="lstClasses" />
								</g:customCell>
							</g:row>
						</g:Grid>
						<g:Label text="." styleName="hiddenText" />
						<p1:DataGrid width="100%" height="500px" ui:field="tblBulletins" />
					</g:VerticalPanel>
				</g:cell>
				<g:VerticalPanel width="100%" height="670px">
					<g:VerticalPanel styleName="subSection" width="100%"
						horizontalAlignment="ALIGN_CENTER">
						<g:Label text="Attribution personnalisée pour l&apos;élève choisi(e)"
							styleName="sectionTitle" />
						<g:Label text="-" ui:field="lblStudentName" styleName="header"
							width="" />
						<g:HorizontalPanel spacing="10" width="100%"
							verticalAlignment="ALIGN_TOP">
							<g:cell width="70%">
								<g:VerticalPanel width="100%" styleName="subSection"
									ui:field="pnlSubject">
									<g:Label text="Liste des matières" styleName="sectionTitle"
										width="" />

									<p1:DataGrid width="100%" height="350px" ui:field="tblSubjects" />

									<g:Label text="." width="450px" styleName="hiddenText"
										height="1px" />
									<g:Cell horizontalAlignment="ALIGN_CENTER">
										<p1:SimplePager location="CENTER" page="0"
											pageStart="0" ui:field="pagerSubjects" />
									</g:Cell>
									<g:VerticalPanel width="100%" ui:field="pnlSubjectAdd">
										<g:Label text="Rajouter une matière" styleName="sectionTitle" />
										<g:Grid width="" cellPadding="2" cellSpacing="2">
											<g:row>
												<g:customCell>
													<g:Label text="Matière :" />
												</g:customCell>
												<g:customCell>
													<g:ListBox ui:field="lstSubjects" width="200px" />
												</g:customCell>
											</g:row>
											<g:row>
												<g:customCell>
													<g:Label text="Professeur 1 :" />
												</g:customCell>
												<g:customCell>
													<g:ListBox ui:field="lstProfessors" width="200px" />
												</g:customCell>
											</g:row>
											<g:row>
												<g:customCell>
													<g:Label text="Professeur 2 :" />
												</g:customCell>
												<g:customCell>
													<g:ListBox width="200px" ui:field="lstProfessors1" />
												</g:customCell>
											</g:row>
											<g:row>
												<g:customCell>
													<g:Label text="Professeur 3 :" />
												</g:customCell>
												<g:customCell>
													<g:ListBox width="200px" ui:field="lstProfessors2" />
												</g:customCell>
											</g:row>
											<g:row>
												<g:customCell>
													<g:Label text="Coefficient :" />
												</g:customCell>
												<g:customCell>
													<g:DoubleBox ui:field="txtSubjectCoef" width="50px" />
												</g:customCell>
											</g:row>
											<g:row>
												<g:customCell>
													<g:Label text="." styleName="hiddenText" />
												</g:customCell>
												<g:customCell>
													<g:Button ui:field="cmdAddSubject" text="Ajouter la matière"
														styleName="buttonToolbar"></g:Button>
												</g:customCell>
												<g:customCell>
													<g:Button text="Valider" ui:field="cmdSaveSubject"
														styleName="buttonToolbar" visible="false" />
												</g:customCell>
											</g:row>
										</g:Grid>
									</g:VerticalPanel>
								</g:VerticalPanel>
							</g:cell>
							<g:VerticalPanel width="100%" styleName="subSection"
								ui:field="pnlBranches">
								<g:Label text="Liste des branches" styleName="sectionTitle" />

								<p1:DataGrid width="100%" height="250px" ui:field="tblBranches" />

								<g:Label text="Rajouter une branche" styleName="sectionTitle"
									width="" />
								<g:VerticalPanel width="100%" ui:field="pnlBrancheAdd">
									<g:Grid cellPadding="5" cellSpacing="5">
										<g:row>
											<g:customCell>
												<g:Label text="Branche :" />
											</g:customCell>
											<g:customCell>
												<g:ListBox ui:field="lstBranches" width="200px"
													styleName="sis_textbox" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Coefficient :" />
											</g:customCell>
											<g:customCell>
												<g:DoubleBox ui:field="txtBrancheCoef" width="50px" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="." styleName="hiddenText" />
											</g:customCell>
											<g:customCell>
												<g:Button text="Ajouter la branche" ui:field="cmdAddBranche"
													styleName="buttonToolbar" />
											</g:customCell>
											<g:customCell>
												<g:Button styleName="buttonToolbar" ui:field="cmdSaveBranche"
													visible="false">Valider</g:Button>
											</g:customCell>
											<g:customCell>
												<g:Label text="." styleName="hiddenText" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:VerticalPanel>
							</g:VerticalPanel>
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
